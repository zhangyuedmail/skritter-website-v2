gelato-page(data-name='mobile-vocablists-mine')

  #dialog-container

  section#vocablist-nav-links.container
    .row.text-center
      if app.getLanguage() === 'zh'
        .col-xs-3.zh-smaller.zh-first
          a(href='/vocablists/queue') Queue
        .col-xs-3.zh-smaller
          a(href='/vocablists/browse') Browse
        .col-xs-3.selected My Lists
        .col-xs-3.zh-last
          a(href='/vocablists/chinesepod') ChinesePod
      else
        .col-xs-4
          a(href='/vocablists/queue') Queue
        .col-xs-4
          a(href='/vocablists/browse') Browse
        .col-xs-4.selected My Lists

  section.container
    #signup-container
    if !app.user.isAnonymous()
      .content-block.active-lists
        .row
          #vocablist-container

  if !app.user.isAnonymous()
    .content-block
      .row
        .header DELETED LISTS

        .col-md-12.main-content
          #deleted-container
